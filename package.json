{
  "name": "matter-surfaces-landing-pages",
  "description": "A typography-heavy & light-themed Gatsby Starter which uses the Headless CMS Prismic",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "author": "Webpoint Solutions, LLC",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -o",
    "develop": "gatsby clean & gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "lint:staged": "eslint --fix --ext .js,.jsx",
    "format": "prettier --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "prettier": "prettier",
    "prettier:fix": "prettier --write \"**/*.{js,jsx}\"",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn prettier --write",
      "yarn lint:staged"
    ],
    "*.{yaml,yml}": [
      "yarn prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@emotion/styled-base": "^11.0.0",
    "@reach/skip-nav": "^0.15.0",
    "emotion": "^11.0.0",
    "emotion-server": "^11.0.0",
    "emotion-theming": "^11.0.0",
    "gatsby": "^4.25.7",
    "gatsby-plugin-emotion": "^6.4.0",
    "gatsby-plugin-google-analytics": "^3.4.0",
    "gatsby-plugin-image": "^1.4.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-netlify": "^3.4.0",
    "gatsby-plugin-offline": "^4.4.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-robots-txt": "^1.5.6",
    "gatsby-plugin-sharp": "^3.4.1",
    "gatsby-plugin-sitemap": "^4.0.0",
    "gatsby-plugin-typography": "^3.4.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.22",
    "gatsby-source-magento2": "^2.4.6",
    "gatsby-source-prismic": "^3.3.6",
    "gatsby-transformer-sharp": "^3.4.0",
    "prismic-dom": "^2.2.5",
    "prismic-reactjs": "^1.3.3",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-slick": "^0.28.1",
    "react-typography": "^0.16.19",
    "slick-carousel": "^1.8.1",
    "typeface-lora": "^1.1.13",
    "typeface-source-sans-pro": "^1.1.13",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cross-env": "^7.0.3",
    "cypress": "^7.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-cypress": "^1.4.0",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.1"
  },
  "keywords": [
    "gatsby",
    "starter",
    "prismic",
    "typography",
    "minimal",
    "gatsby-starter"
  ]
}
